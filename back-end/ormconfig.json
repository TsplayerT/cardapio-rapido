{
   "type": "sqlite",
   "database": "bancodados.db",
   "synchronize": true,
   "logging": false,
   "entities": [
      "entity/**/*.ts"
   ],
   "migrations": [
      "migration/**/*.ts"
   ],
   "subscribers": [
      "subscriber/**/*.ts"
   ],
   "cli": {
      "entitiesDir": "entity",
      "migrationsDir": "migration",
      "subscribersDir": "subscriber"
   }
}